<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="189.0" prefWidth="393.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.fsp.gitfinder.controleur.ConfigGitPathControleur">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label text="Saissisez le chemin vers Git" />
   <TextField fx:id="gitPath" promptText="Chemin vers git" />
   <HBox alignment="CENTER" prefHeight="37.0" prefWidth="353.0" spacing="30.0">
      <children>
          <Button onAction="#onConfirmerClick" text="Valider" />
         <Button onAction="#onParcourirClick" mnemonicParsing="false" text="Parcourir..." />
      </children>
   </HBox>
</VBox>
